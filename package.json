{
  "name": "airooter",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently -n server,web -c blue,green \"npm:dev:server\" \"npm:dev:web\"",
    "dev:server": "nodemon server/index.js",
    "dev:web": "vite",
    "build": "vite build",
    "build:vercel": "npm run build && npm run prepare:api",
    "prepare:api": "rm -rf api/server && mkdir -p api && cp -R server api/",
    "start": "node server/index.js",
    "preview": "NODE_ENV=production node server/index.js",
    "vercel-build": "npm run build && npm run prepare:api",
    "check:env": "node scripts/check_env.mjs"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.925.0",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^6.1.3",
    "@mui/material": "^6.1.3",
    "axios": "^1.7.7",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "morgan": "^1.10.0",
    "node-fetch": "^3.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.3.2",
    "nodemon": "^3.0.1",
    "vite": "^5.4.10"
  }
}
